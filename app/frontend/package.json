{
    "name": "finconsult",
    "description": "Finconsult v2 SPA",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "ng": "ng",
        "serve": "ng serve",
        "dev": "ng build --base-href=\"/app/\" --output-path=\"../public/app\" --watch --aot --build-optimizer --optimization",
        "prettier": "npx prettier --check \"./src/**/*.ts\"",
        "prettify": "npx prettier --write \"./src\"",
        "codegen": "graphql-codegen --config codegen.yml",
        "test": "ng test",
        "prod": "npm run prettier && npm run codegen && ng build --base-href=\"/app/\" --aot --build-optimizer --optimization",
        "lint": "ng lint"
    },
    "dependencies": {
        "@angular/animations": "~13.3.0",
        "@angular/cdk": "^13.3.2",
        "@angular/common": "~13.3.0",
        "@angular/compiler": "~13.3.0",
        "@angular/core": "~13.3.0",
        "@angular/forms": "~13.3.0",
        "@angular/platform-browser": "~13.3.0",
        "@angular/platform-browser-dynamic": "~13.3.0",
        "@angular/router": "~13.3.0",
        "@apollo/client": "^3.0.0",
        "@fortawesome/fontawesome-free": "^6.0.0",
        "@graphql-tools/webpack-loader": "^6.7.0",
        "apollo-angular": "^3.0.1",
        "chart.js": "^3.1.1",
        "graphql": "16.5.0",
        "mdb-angular-ui-kit": "^2.1.0",
        "rxjs": "~7.5.0",
        "tslib": "^2.3.0",
        "zone.js": "~0.11.4"
